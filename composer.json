{
    "name": "freshbitsweb/laravel-google-analytics-4-measurement-protocol",
    "description": "A Laravel package to use Measurement Protocol for Google Analytics 4",
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "illuminate/http": "^7.0|^8.0|^9.0|^10.0",
        "illuminate/view": "^7.0|^8.0|^9.0|^10.0",
        "spatie/laravel-package-tools": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "license": "MIT",
    "keywords": [
        "freshbitsweb",
        "laravel-google-analytics-4-measurement-protocol"
    ],
    "homepage": "https://github.com/freshbitsweb/laravel-google-analytics-4-measurement-protocol",
    "authors": [
        {
            "name": "Gaurav Makhecha",
            "email": "info@freshbits.in",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Freshbitsweb\\LaravelGoogleAnalytics4MeasurementProtocol\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Freshbitsweb\\LaravelGoogleAnalytics4MeasurementProtocol\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Freshbitsweb\\LaravelGoogleAnalytics4MeasurementProtocol\\GA4ServiceProvider"
            ],
            "aliases": {
                "GA4": "Freshbitsweb\\LaravelGoogleAnalytics4MeasurementProtocol\\Facades\\GA4"
            }
        }
    }
}
